{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "A Webpack boilerplate with hot reloading React components, and error handling on module and component level.",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "package": "zip -r pkg.$PKG_ID.zip index.html dist -x '*.zip'",
    "dist": "zip --symlinks -r dist.$DIST_ID.zip * -x '.git*' -x '*.zip'",
    "start": "node devServer.js",
    "lint": "eslint src",
    "test": "cross-env NODE_ENV=test mocha --recursive --compilers js:babel-register --require ./test/unit/utils.js",
    "test:watch": "npm test -- --watch",
    "e2e": "nightwatch -c nightwatch.e2e.conf.js",
    "e2e:smoke": "nightwatch -c nightwatch.e2e.conf.js --group smoke",
    "e2e:dashboard": "nightwatch -c nightwatch.e2e.conf.js --group landing",
    "sitespeed:desktop": "sitespeed.io --profile desktop --screenshot --suppressDomainFolder --outputFolderName desktop",
    "sitespeed:mobile": "sitespeed.io --profile mobile --screenshot --suppressDomainFolder --outputFolderName mobile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vspiewak/react-redux-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Vincent Spiewak <vspiewak@the_google_mail_you_know> (http://github.com/vspiewak)",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/vspiewak/react-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/vspiewak/react-redux-boilerplate",
  "devDependencies": {
    "babel-core": "^6.6.5",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.5.0",
    "chromedriver": "^2.21.2",
    "cross-env": "^1.0.7",
    "enzyme": "^2.1.0",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "nightwatch": "^0.8.18",
    "phantomjs-prebuilt": "^2.1.6",
    "react-addons-test-utils": "^0.14.7",
    "rimraf": "^2.4.3",
    "selenium-server-standalone-jar": "^2.53.0",
    "sitespeed.io": "^3.11.5",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.9.1"
  },
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  }
}
