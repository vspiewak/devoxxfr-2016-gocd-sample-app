{
  "name": "react-redux-boilerplate",
  "version": "1.0.0",
  "description": "A Webpack boilerplate with hot reloading React components, and error handling on module and component level.",
  "scripts": {
    "clean": "rimraf dist",
    "check-update": "ncu",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "package": "zip -r pkg.$PKG_ID.zip index.html dist -x '*.zip'",
    "dist": "zip --symlinks -r dist.$DIST_ID.zip * -x '.git*' -x '*.zip'",
    "start": "node devServer.js",
    "lint": "eslint src",
    "test": "cross-env NODE_ENV=test mocha --recursive --compilers js:babel-register --require ./test/unit/utils.js",
    "test:watch": "npm test -- --watch",
    "e2e": "nightwatch -c nightwatch.e2e.conf.js",
    "e2e:smoke": "nightwatch -c nightwatch.e2e.conf.js --group smoke",
    "e2e:landing": "nightwatch -c nightwatch.e2e.conf.js --group landing",
    "bdd": "nightwatch -c nightwatch.bdd.conf.js",
    "bdd:smoke": "nightwatch -c nightwatch.bdd.conf.js --group smoke",
    "bdd:landing": "nightwatch -c nightwatch.bdd.conf.js --group landing",
    "coverage": "babel-node node_modules/isparta/bin/isparta cover _mocha --include-all-sources --report lcov --require ./test/unit/utils.js",
    "plato": "plato -r -d reports src",
    "sitespeed:desktop": "sitespeed.io --profile desktop --screenshot --suppressDomainFolder --outputFolderName sitespeed.io-desktop",
    "sitespeed:mobile": "sitespeed.io --profile mobile --screenshot --suppressDomainFolder --outputFolderName sitespeed.io-mobile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vspiewak/react-redux-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Vincent Spiewak <vspiewak@the_google_mail_you_know> (http://github.com/vspiewak)",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/vspiewak/react-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/vspiewak/react-redux-boilerplate",
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-core": "6.7.4",
    "babel-eslint": "6.0.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "chai": "3.5.0",
    "chromedriver": "2.21.2",
    "cross-env": "1.0.7",
    "cucumber": "0.9.5",
    "enzyme": "2.2.0",
    "eslint": "2.5.1",
    "eslint-plugin-babel": "3.1.0",
    "eslint-plugin-react": "4.2.3",
    "eventsource-polyfill": "0.9.6",
    "express": "4.13.4",
    "isparta": "4.0.0",
    "jsdom": "8.1.0",
    "mocha": "2.4.5",
    "nightwatch": "0.8.18",
    "nightwatch-cucumber": "2.1.3",
    "npm-check-updates": "2.6.1",
    "phantomjs-prebuilt": "2.1.7",
    "plato": "git+https://github.com/deedubs/es6-plato.git",
    "react-addons-test-utils": "0.14.7",
    "rimraf": "2.5.2",
    "selenium-server-standalone-jar": "2.53.0",
    "sitespeed.io": "3.11.5",
    "webpack": "1.12.14",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0"
  },
  "dependencies": {
    "react": "0.14.7",
    "react-dom": "0.14.7"
  }
}
